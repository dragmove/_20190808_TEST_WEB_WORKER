{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "jquery": true,
    "amd": true,
    "es6": true
  },
  "globals": {},
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "constructor-super": "off",
    "indent": "off",
    "linebreak-style": "off",
    "no-console": "off",
    "no-extra-boolean-cast": "off",
    "no-unreachable": "off",
    "no-unused-vars": [
      "error",
      {
        "args": "none"
      }
    ],
    "quotes": "off",
    "semi": ["error", "always"]
  }
}
